{
  "name": "senplades",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "eslint:report": "npx eslint . -f node_modules/eslint-detailed-reporter/lib/detailed.js -o eslin-report.html",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "sonarqube": "sonar-scanner"
  },
  "dependencies": {
    "@types/react-dom": "18.0.9",
    "axios": "^1.1.3",
    "date-fns": "^2.29.3",
    "eslint-config-next": "13.0.3",
    "file-saver": "^2.0.5",
    "formik": "^2.2.9",
    "google-map-react": "^2.2.0",
    "gotham-fonts": "^1.0.3",
    "js-file-download": "^0.4.12",
    "leaflet": "^1.9.3",
    "next": "13.0.3",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primereact": "^8.7.2",
    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-detailed-reporter": "^0.9.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.2",
    "jsdoc": "^4.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "react-json-pretty": "^2.2.0",
    "sonar-scanner": "^3.1.0",
    "typescript": "^4.9.3"
  }
}
